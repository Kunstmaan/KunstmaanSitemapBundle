{
    "name": "kunstmaan/sitemap-bundle",
    "type": "symfony-bundle",
    "description": "A sitemap bundle to generate a sitemap for your Kunstmaan bundles website based on its Nodes",
    "keywords": [ "kunstmaan", "cms", "sitemap"],
    "homepage": "https://github.com/Kunstmaan/KunstmaanSitemapBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kunstmaan Team",
            "homepage": "http://www.kunstmaan.be/",
            "email": "support@kunstmaan.be"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": ">=2.3.0,<2.4.0",
        "kunstmaan/admin-bundle": "2.3.*",
        "kunstmaan/node-bundle": "2.3.*"
    },
    "require-dev": {
        "doctrine/data-fixtures": "1.0.*@dev",
        "doctrine/doctrine-fixtures-bundle": "2.1.*@dev",
        "friendsofsymfony/user-bundle": "dev-master",
        "knplabs/knp-menu": "2.0.0-alpha1@dev",
        "knplabs/knp-menu-bundle": "2.0.0-alpha1@dev",
        "pagerfanta/pagerfanta": "dev-master",
        "symfony-cmf/routing": "~1.1.0-dev",
        "symfony-cmf/routing-bundle": "1.1.x-dev",
        "white-october/pagerfanta-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": { "Kunstmaan\\SitemapBundle": "" }
    },
    "target-dir": "Kunstmaan/SitemapBundle"
}
